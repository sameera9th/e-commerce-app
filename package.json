    "babel-runtime": "^6.26.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.1.2",
    "lint-staged": "^8.0.4",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0"
